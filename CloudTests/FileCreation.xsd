  <xs:simpleType name="FileOperationType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Create" />
      <xs:enumeration value="Rename" />
      <xs:enumeration value="Delete" />
      <xs:enumeration value="LockedModify" />
      <xs:enumeration value="UnlockedAppend" />
      <xs:enumeration value="Stream" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="ChangedData">
    <xs:all minOccurs="1" maxOccurs="1">
      <xs:element name="SizeDelta" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" />
      <xs:element name="SpecificData" type="xs:string" minOccurs="0" maxOccurs="1" />
    </xs:all>
  </xs:complexType>
  <xs:complexType name="SingleFileOperation">
    <xs:all>
      <xs:element name="NewRelativePath" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="OldRelativePath" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="IsFolder" type="xs:boolean" minOccurs="1" maxOccurs="1" />
      <xs:element name="Type" type="FileOperationType" minOccurs="1" maxOccurs="1" />
      <xs:element name="Data" type="ChangedData" minOccurs="0" maxOccurs="1" />
      <xs:element name="StreamIterations" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" />
      <xs:element name="MillisecondDelayBetweenStreamIterations" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" />
      <xs:element name="WaitOnStreamCompletion" type="xs:boolean" minOccurs="0" maxOccurs="1" />
      <xs:element name="MinimumWaitOnLockedModify" type="xs:int" minOccurs="0" maxOccurs="1" />
    </xs:all>
  </xs:complexType>
  <xs:complexType name="ParallelFileOperationSet">
    <xs:choice minOccurs="1" maxOccurs="unbounded">
