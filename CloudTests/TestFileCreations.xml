<?xml version="1.0" encoding="utf-8"?>
<FileCreation xmlns="http://Cloud.com/FileCreation.xsd">
  <Copyright>
    <FileName>TestFileCreations.xml</FileName>
    <Copyright>Implementation of FileCreation.xsd XML Schema. Cloud. Copyright (c) Cloud.com. All rights reserved.</Copyright>
    <Creator>DavidBruck</Creator>
  </Copyright>
  <ParallelFileOperationSet>
    <SingleFileOperation>
      <NewRelativePath>MyFolder</NewRelativePath>
      <IsFolder>true</IsFolder>
      <Type>Create</Type>
    </SingleFileOperation>
  </ParallelFileOperationSet>
  <ParallelFileOperationSet>
    <SingleFileOperation>
      <NewRelativePath>MyFolder\StreamingFile.txt</NewRelativePath>
      <IsFolder>false</IsFolder>
      <Type>Stream</Type>
      <Data>
        <SpecificData>AppendStream</SpecificData>
      </Data>
      <MillisecondDelayBetweenStreamIterations>50</MillisecondDelayBetweenStreamIterations>
      <StreamIterations>5000</StreamIterations>
    </SingleFileOperation>
    <SingleFileOperation>
      <NewRelativePath>UnlockedFile.txt</NewRelativePath>
      <IsFolder>false</IsFolder>
      <Type>UnlockedAppend</Type>
      <Data>
        <SpecificData>AppendUnlocked</SpecificData>
      </Data>
    </SingleFileOperation>
  </ParallelFileOperationSet>
  <ParallelFileOperationSet>
    <SingleFileOperation>
      <NewRelativePath>File1.txt</NewRelativePath>
      <IsFolder>false</IsFolder>
      <Type>LockedModify</Type>
      <Data>
        <SpecificData>AppendLocked</SpecificData>
      </Data>
      <MinimumWaitOnLockedModify>1000</MinimumWaitOnLockedModify>
    </SingleFileOperation>
    <SingleFileOperation>
      <NewRelativePath>MyFolder\File1.txt</NewRelativePath>
      <IsFolder>false</IsFolder>
      <Type>Create</Type>
      <Data>
        <SpecificData>Created</SpecificData>
      </Data>
    </SingleFileOperation>
  </ParallelFileOperationSet>
  <ParallelFileOperationSet>
    <SingleFileOperation>
      <NewRelativePath>File1.txt</NewRelativePath>
      <OldRelativePath>MyFolder\File1.txt</OldRelativePath>
      <IsFolder>false</IsFolder>
      <Type>Rename</Type>
    </SingleFileOperation>
  </ParallelFileOperationSet>
  <ParallelFileOperationSet>
    <SingleFileOperation>
      <NewRelativePath>File1.txt</NewRelativePath>
      <IsFolder>false</IsFolder>
      <Type>UnlockedAppend</Type>
      <Data>
        <SpecificData>AppendAfterRename</SpecificData>
      </Data>
    </SingleFileOperation>
  </ParallelFileOperationSet>
</FileCreation>