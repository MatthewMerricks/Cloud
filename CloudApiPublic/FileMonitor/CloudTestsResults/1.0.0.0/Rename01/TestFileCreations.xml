  </ParallelFileOperationSet>
  <ParallelFileOperationSet>
    <SingleFileOperation>
      <NewRelativePath>file1.txt</NewRelativePath>
      <OldRelativePath>MyFolder\file1.txt</OldRelativePath>
      <IsFolder>false</IsFolder>
      <Type>Rename</Type>
    </SingleFileOperation>
  </ParallelFileOperationSet>
  <ParallelFileOperationSet>
    <SingleFileOperation>
      <NewRelativePath>file.txt</NewRelativePath>
      <OldRelativePath>file1.txt</OldRelativePath>
      <IsFolder>false</IsFolder>
      <Type>Rename</Type>
    </SingleFileOperation>
    <SingleFileOperation>
      <NewRelativePath>AnotherFolder</NewRelativePath>
      <OldRelativePath>MyFolder</OldRelativePath>
      <IsFolder>true</IsFolder>
      <Type>Rename</Type>
    </SingleFileOperation>
  </ParallelFileOperationSet>
  <ParallelFileOperationSet>
    <SingleFileOperation>
      <NewRelativePath>AnotherFolder\file.txt</NewRelativePath>
      <OldRelativePath>file.txt</OldRelativePath>
