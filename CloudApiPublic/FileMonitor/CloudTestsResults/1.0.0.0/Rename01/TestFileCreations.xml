  <ParallelFileOperationSet>
    <SingleFileOperation>
      <NewRelativePath>MyFolder</NewRelativePath>
      <IsFolder>true</IsFolder>
      <Type>Create</Type>
    </SingleFileOperation>
    <SingleFileOperation>
      <NewRelativePath>file1.txt</NewRelativePath>
      <IsFolder>false</IsFolder>
      <Type>Create</Type>
      <Data>
        <SpecificData>AddMe</SpecificData>
      </Data>
    </SingleFileOperation>
  </ParallelFileOperationSet>
  <ParallelFileOperationSet>
    <SingleFileOperation>
      <NewRelativePath>MyFolder\file1.txt</NewRelativePath>
      <OldRelativePath>file1.txt</OldRelativePath>
      <IsFolder>false</IsFolder>
      <Type>Rename</Type>
