HKCR
{
	BadgeCOM.BadgeIconSyncing.1 = s 'BadgeIconSyncing Class'
	{
		CLSID = s '%IconSyncingGuid%'
	}
	BadgeCOM.BadgeIconSyncing = s 'BadgeIconSyncing Class'
	{
		CLSID = s '%IconSyncingGuid%'
		CurVew = s 'BadgeCOM.BadgeIconSyncing.1'
	}
	NoRemove CLSID
	{
		ForceRemove %IconSyncingGuid% = s 'BadgeIconSyncing Class'
		{
			ProgID = s 'BadgeCOM.BadgeIconSyncing.1'
			VersionIndependentProgID = s 'BadgeCOM.BadgeIconSyncing'
			ForceRemove Programmable
			InprocServer32 = s '%MODULE%'
			{
				val ThreadingModel = s 'Apartment'
			}
			val AppID = s '%APPID%'
			TypeLib = s '%TypeLibGuid%'
			Version = s '1.0'
		}
	}
}

HKLM
{
  NoRemove SOFTWARE
  {
    NoRemove Microsoft
    {
      NoRemove Windows
      {
        NoRemove CurrentVersion
        {
          NoRemove Explorer
          {
            NoRemove ShellIconOverlayIdentifiers
            {
              ForceRemove BadgeIconSyncing = s '%IconSyncingGuid%'
              {
              }
            }
          }
        }
      }
    }
  }
}