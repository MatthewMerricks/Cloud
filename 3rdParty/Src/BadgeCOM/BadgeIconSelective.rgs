	{
		CLSID = s '%IconSelectiveGuid%'
		CurVer = s 'BadgeCOM.BadgeIconSelective.1'
	}
	NoRemove CLSID
	{
		ForceRemove %IconSelectiveGuid% = s 'BadgeIconSelective Class'
		{
			ProgID = s 'BadgeCOM.BadgeIconSelective.1'
			VersionIndependentProgID = s 'BadgeCOM.BadgeIconSelective'
			ForceRemove Programmable
			InprocServer32 = s '%MODULE%'
			{
				val ThreadingModel = s 'Apartment'
			}
			val AppID = s '%APPID%'
			TypeLib = s '%TypeLibGuid%'
			Version = s '1.0'
		}
	}
}

HKLM
{
  NoRemove SOFTWARE
  {
    NoRemove Microsoft
    {
      NoRemove Windows
      {
        NoRemove CurrentVersion
        {
          NoRemove Explorer
          {
            NoRemove ShellIconOverlayIdentifiers
            {
              ForceRemove BadgeIconSelective = s '%IconSelectiveGuid%'
              {
              }
            }
          }
        }
      }
    }
  }
}