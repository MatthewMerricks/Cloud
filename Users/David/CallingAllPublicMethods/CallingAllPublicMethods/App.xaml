             xmlns:viewModels="clr-namespace:CallingAllPublicMethods.ViewModels"
             xmlns:converters="clr-namespace:CallingAllPublicMethods.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <viewModels:MainViewModel x:Key="MainViewModel" />
        <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
        <converters:NullableLongToStringConverter x:Key="NullableLongToStringConverter" />
        <converters:CombineSyncboxViewModelWithSelectedSyncboxConverter x:Key="CombineSyncboxViewModelWithSelectedSyncboxConverter" />
    </Application.Resources>
</Application>
